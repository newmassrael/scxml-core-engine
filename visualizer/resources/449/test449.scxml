<?xml version="1.0"?>
<!--  test that ecmascript objects are converted to booleans inside cond -->


<scxml  datamodel="ecmascript" version="1.0"  xmlns="http://www.w3.org/2005/07/scxml">
 
     
<state id="s0">
  <transition  cond="'foo'" target="pass"/>
  <transition  target="fail"/>
    </state>

<final id="pass"/>
<final id="fail"/>
</scxml>