<?xml version="1.0"?>

<!-- test that Basic HTTP messages go into external queue.   -->

<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">
  
<state id="s0">
  <onentry>
    <send event="timeout" delay="30s"/>
    <send event="test" target="http://localhost:8080/test" type="http://www.w3.org/TR/scxml/#BasicHTTPEventProcessor"/>
     <!-- this creates an internal event -->
     <raise event="internal"/>
  
      </onentry>
     <!-- we should get 'internal' first, then 'test' -->
  <transition event="internal" target="s1"/>
  <transition event="*" target="fail"/>
 </state>
 
 <state id="s1">
   <transition event="test" target="pass"/>
   <transition event="*" target="fail"/>
   </state>
  
   <final id="pass"/>
   <final id="fail"/>

</scxml>