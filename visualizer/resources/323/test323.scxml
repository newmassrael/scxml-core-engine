<?xml version="1.0"?>

<!-- test that _name is bound on startup -->

<scxml initial="s0" version="1.0" xmlns="http://www.w3.org/2005/07/scxml" datamodel="ecmascript"  name="machineName">
<datamodel>
  <data id="Var1" expr="_name"/>
  </datamodel>
     
 <state id="s0">
<transition cond="typeof Var1 !== 'undefined'" target="pass"/> 
   <transition cond="true" target="fail"/>
   </state>
   
    
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>