<!-- test that that if _scxmleventname  is not present, the name of the HTTP method is used
as the name of the resulting event. -->

<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">

   
<state id="s0">
  <onentry>
    <send event="timeout" delay="3s"/>
    <send   target="http://localhost:8080/test" type="http://www.w3.org/TR/scxml/#BasicHTTPEventProcessor">
    <!-- this content will be ignored, but it's here to make sure we have a message body -->
      <content>some content</content>
      </send>  
     </onentry>
     
  <transition event="HTTP.POST" target="pass"/>
  <transition event="*" target="fail"/>
 </state>
  
   <final id="pass"/>
   <final id="fail"/>
</scxml>