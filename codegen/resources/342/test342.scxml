<?xml version="1.0"?>

<!-- test that eventexpr works and sets the name field of the resulting event -->
<scxml initial="s0" version="1.0" datamodel="ecmascript"  xmlns="http://www.w3.org/2005/07/scxml"  name="machineName">  
 <datamodel>
   <data id="Var1" expr="'foo'"/>
   <data id="Var2"/>
   </datamodel>
     
 <state id="s0">
   <onentry>
     <send  eventexpr="Var1"/>
   </onentry>
   <transition event="foo" target="s1">
      <assign location="Var2" expr="_event.name"/>
      </transition>
      <transition event="*" target="fail"/> 
   </state>
   
<state id="s1">
  <transition cond="Var1 === Var2" target="pass"/>
   <transition target="fail"/>
   </state>
      
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>