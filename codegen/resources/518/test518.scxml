<?xml version="1.0"?>

<!-- test that that namelist values get encoded as POST parameters.  -->

<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">
<datamodel>
  <data id="Var1" expr="2"/>
  </datamodel>

<state id="s0">
  <onentry>
    <send event="timeout" delay="30s"/>
    <send  event="test" target="http://localhost:8080/test" namelist="Var1"
    type="http://www.w3.org/TR/scxml/#BasicHTTPEventProcessor"/>
      </onentry>
     
  <transition event="test" target="pass"/>
  <transition event="*" target="fail"/>

 </state>
 
   <final id="pass"/>
   <final id="fail"/>

</scxml>