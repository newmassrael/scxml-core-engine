<?xml version="1.0"?>
<!-- test illegal assignment.  error.execution should be raised.  -->


<scxml  initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">
<datamodel>
  <data id="Var1"/>
  </datamodel>
     
<state id="s0">
  <onentry>
  	<assign location="Var1" expr="undefined.invalidProperty"/>
   <raise event="event"/>
   </onentry>
   
  <transition event="error.execution" target="pass"/>
  <transition event="*" target="fail"/>
</state>
 
<final id="pass"/>
<final id="fail"/>
</scxml>