<?xml version="1.0"?>

<!-- make sure sendid is blank in a non-error event -->

<scxml version="1.0" xmlns="http://www.w3.org/2005/07/scxml" initial="s0" datamodel="ecmascript"  name="machineName">  
   
 <state id="s0">
   <onentry>
     <send  event="foo"/>
   </onentry>
   <transition event="foo" target="pass"/>
      <transition event="*" target="fail"/> 
   </state>
   

      
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>