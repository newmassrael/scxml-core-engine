<?xml version="1.0"?>
<!-- we test that the processor supports the scxml event i/o processor -->


<scxml initial="s0" datamodel="ecmascript" version="1.0" xmlns="http://www.w3.org/2005/07/scxml">

<state id="s0">
  <onentry>
   <send type="http://www.w3.org/TR/scxml/#SCXMLEventProcessor" event="event1"/>
   <send event="timeout"/>
    </onentry>
 
 
  <transition event="event1" target="pass"/>
  <transition event="*" target="fail"/>
 </state>
 

<final id="pass"/>
<final id="fail"/>

</scxml>