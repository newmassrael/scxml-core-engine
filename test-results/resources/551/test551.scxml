<?xml version="1.0"?>

<!-- test that inline content can be used to assign a value to a var.  -->

<scxml initial="s0" version="1.0" binding="early" datamodel="ecmascript"  xmlns="http://www.w3.org/2005/07/scxml">

    
   <state id="s0">
 
       <transition cond="typeof Var1 !== 'undefined'" target="pass"/>
       <transition  target="fail"/>
   </state>
   
<state id="s1">
	  <datamodel>
    <data id="Var1">
     [1,2,3]
     </data>
    </datamodel>
    </state>

   <final id="pass"/>
   <final id="fail"/>
   


</scxml>