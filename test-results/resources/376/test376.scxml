<?xml version="1.0"?>
<!-- test that each onentry handler is a separate block.  The <send> of event1 will cause an error but
 the increment to var1 should happen anyways -->


<scxml  datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">
<datamodel>
  <data id="Var1" expr="1"/>
 </datamodel>
       
<state id="s0">
  <onentry>
    <send target="!invalid" event="event1"/>
    </onentry>
  <onentry>
    <assign location="Var1" expr="Var1 + 1"/>
    </onentry>
    
  <transition  cond="Var1 == 2" target="pass"/>
  <transition  target="fail"/>

</state>
 
<final id="pass"/>
<final id="fail"/>
</scxml>