<?xml version="1.0"?>

<!-- test that _name stays bound till the session ends.  This means that it cannot be assigned to -->

<scxml initial="s0" version="1.0" xmlns="http://www.w3.org/2005/07/scxml" datamodel="ecmascript"  name="machineName">

     
 <state id="s0">
   <transition target="s1"/>
    <transition target="fail"/>
   </state>
   
 <state id="s1">
   <onentry>
    <assign location="_name" expr="'otherName'"/>
    </onentry>
   <transition target="pass"/>
   <transition target="fail"/>
   </state>  
   
   
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>