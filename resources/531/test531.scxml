<!-- test that that the value of the <param> _scxmleventname gets used as the name
of the raised event. -->

<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">

   
<state id="s0">
  <onentry>
    <send event="timeout" delay="3s"/>
    <send   target="http://localhost:8080/test" type="http://www.w3.org/TR/scxml/#BasicHTTPEventProcessor">
      <param name="_scxmleventname" expr="'test'"/>
      </send>  
     </onentry>
     
     <!-- if we get an event named 'test' we succeed. Otherwise fail -->
  <transition event="test" target="pass"/>
  <transition event="*" target="fail"/>
 </state>
  
   <final id="pass"/>
   <final id="fail"/>
</scxml>