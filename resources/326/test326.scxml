<?xml version="1.0"?>

<!-- test that _ioprocessors stays bound till the session ends.  This means that it cannot be assigned to -->
<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml" name="machineName">
<datamodel>
  <data id="Var1" expr="_ioprocessors"/>
  <data id="Var2"/>
  </datamodel>

 <state id="s0">
    <transition cond="typeof Var1 !== 'undefined'" target="s1"/> 
   <transition cond="true" target="fail"/>
   </state> 
      
   
 <state id="s1">
   <onentry>
    <assign location="_ioprocessors" expr="'otherName'"/>
    <raise event="foo"/>
    </onentry>
 
   <transition event="error.execution" target="s2"/>
   <transition event="*" target="fail"/>
   </state>
   
 <state id="s2">
   <onentry>
     <assign location="Var2" expr="_ioprocessors"/>
     </onentry>
   <transition target="pass"/>
   <transition target="fail"/>
   </state>  
   
   
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>