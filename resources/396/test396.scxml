<?xml version="1.0"?>
<!-- test that the value in _event.name matches the event name used to match against transitions  -->


<scxml  datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">
       
<state id="s0">
  <onentry>
    <raise event="foo"/>
    </onentry>

    
  <transition  event="foo" target="pass"/>
  <transition  event="foo" target="fail"/>

</state>
 
<final id="pass"/>
<final id="fail"/>
</scxml>