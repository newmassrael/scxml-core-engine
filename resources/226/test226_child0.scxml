<?xml version="1.0"?>
<!-- when invoked, if var1 has a value notify parent. Then terminate.   -->


<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">

<datamodel>
  <data id="Var1"/>
  </datamodel>

<state id="s0">
  <transition cond="typeof Var1 !== 'undefined'" target="final">
    <send target="#_parent" event="varBound"/>
    </transition>
  <transition target="final"/>
  </state>
  
<final id="final"/>

</scxml>