<?xml version="1.0"?>

<!-- check that the required fields are present in both internal and external events -->

<scxml version="1.0" initial="s0" datamodel="ecmascript"  name="machineName" xmlns="http://www.w3.org/2005/07/scxml">
     
 <state id="s0">
   <onentry>
     <raise event="foo"/>
   </onentry>
   <transition event="foo" target="s1"/> 
      <transition event="*" target="fail"/> 
   </state>
   
 <state id="s1">
   <onentry>
     <send event="foo"/>
   </onentry>
   <transition event="foo" target="pass"/> 
      <transition event="*" target="fail"/> 
   </state>
   
    
   <final id="pass"/>
   <final id="fail"/>
     
</scxml>