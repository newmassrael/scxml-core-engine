<?xml version="1.0"?>

<scxml initial="s0" datamodel="ecmascript"  version="1.0" xmlns="http://www.w3.org/2005/07/scxml">

 <!-- test that event param of send sets the name of the event -->
  
<state id="s0">

   <onentry>
     <send  type="http://www.w3.org/TR/scxml/#SCXMLEventProcessor" event="s0Event"/>
     </onentry>
  <transition event="s0Event" target="pass"/>
  <transition event="*" target="fail"/>

</state>
  
   <final id="pass"/>
   <final id="fail"/>

</scxml>